{
  "name": "supertest",
  "version": "1.0.0",
  "description": "A basic example for the usage of supertest in API testing",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "devServer": "cross-env NODE_ENV=development nodemon server.js",
    "testServer": "cross-env NODE_ENV=test nodemon server.js",
    "initData": "cross-env NODE_ENV=development node config/initData.js",
    "initTestData": "cross-env NODE_ENV=test node config/initData.js",
    "test": "cross-env NODE_ENV=test jest --verbose -runInBand"
  },
  "keywords": [],
